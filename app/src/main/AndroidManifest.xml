<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycafediary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".DiaryWriteActivity"></activity>
        <activity android:name=".DiaryViewActivity" />
        <activity android:name=".DiaryListActivity" />
        <activity android:name=".PrivacyActivity" />
        <activity android:name=".TermsActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".SplashActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>